{
  "name": "myapp",
  "version": "0.0.0",
  "engines": {
    "node": "8.11.3",
    "npm": "5.6.0"
  },
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "debug": "node --inspect-brk=9229 app.js",
    "worker": "pm2 start ./bin/worker.js -i 0 --name 'worker'",
    "batch": "export NODE_ENV=staging && pm2 start ./app.js --name 'batch'",
    "prod": "pm2 start ./app.js --name 'batch' && pm2 start ./bin/worker.js --name 'worker'",
    "build": "pkg . --out-path=dist --debug",
    "deploy": "git pull && npm install && pm2 start ecosystem.config.js --env"
  },
  "dependencies": {
    "@slack/client": "^4.8.0",
    "aigle": "^1.12.0",
    "async-foreach": "^0.1.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "config": "^2.0.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "^4.16.4",
    "express-bearer-token": "^2.2.0",
    "express-session": "^1.15.6",
    "fcm-node": "^1.3.0",
    "fs": "0.0.1-security",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "mongoose": "^5.3.4",
    "morgan": "^1.9.1",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.1",
    "node-fetch": "^2.3.0",
    "node-schedule": "^1.3.0",
    "node-telegram-bot-api": "^0.30.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "request": "^2.88.0",
    "sequelize": "^4.39.1",
    "sequelize-cli": "^4.1.1",
    "swagger-ui-express": "^4.0.1",
    "sync-request": "^6.0.0",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.3.3",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1"
  }
}
